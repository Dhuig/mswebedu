.author-pop .subtitle{ line-height: 1.2; color: #000; }
.author-pop .author-list{ padding-top: 20rpx; }
.author-pop .author-list .list{ position: relative; margin-bottom: 8rpx; line-height: 1.2; font-size: 24rpx; color: #999; }
.author-pop .author-list .list:before{ content: ""; display:inline-block; width: 8rpx; height: 8rpx; margin-right: 6rpx; border-radius: 50%; vertical-align: middle; background:#eee;  }

/** 志愿详情 **/
.view-table-layout{ margin: 0 12rpx; border:2rpx solid #eee; line-height: 1.2; border-radius: 6rpx; }
.view-table-layout .thead{padding: 20rpx 0;border-bottom: 2rpx solid #eee; text-align:center;}
.view-table-layout .thead:nth-child(1) .th{ color: #666; }
.view-table-layout .thead .th{ float: left; width: 150rpx;  }
.view-table-layout .thead .th:nth-child(1){ width: 120rpx;}
.view-table-layout .thead .th:nth-child(2){ width: 300rpx; text-align: left; }
.view-table-layout .tfooter{ position: relative; min-height:70rpx; border-bottom: 2rpx solid #eee; }
.view-table-layout .tfooter:last-child{ border-bottom: none; color: #999; }
.view-table-layout .tfooter .subtitle{ position: absolute; left: 0; top:0; bottom: 0; width: 180rpx;  border-right: 2rpx solid #eee;  text-align:center; }
.view-table-layout .tfooter .subtitle text{ position: absolute; width: 100%; left: 0; top: 50%; margin-top: -14rpx; }
.view-table-layout .tfooter .td{ margin-left: 180rpx; padding: 20rpx 0;  border-bottom: 2rpx solid #eee; text-indent: 20rpx; }
.view-table-layout .tfooter .td:last-child{ border-bottom: none; }
.view-table-layout .tfooter:last-child td{color: #999;}
.view-table-layout .icon-tag{ display: inline-block; margin: 0 6rpx 6rpx 0; padding: 4rpx; vertical-align: middle; font-size: 24rpx; border-radius: 8rpx; color: #fff; background: #f60; }
.view-table-layout .icon-tag:nth-child(2){ background: #19ad19; }
.view-table-layout .icon-tag:nth-child(3){ background: #f00; }
.ui-hr-line{ display: block; margin: 30rpx 12rpx; border-bottom: 10rpx solid #ddd; }




/** 去掉拼团按钮 2018-06-13 **/
.ui-btn-group[data-state="0"][data-status="999"] .ui-btn-block:nth-child(1){ display: none; }
.ui-btn-group[data-state="0"][data-status="999"] .ui-btn-block:nth-child(2){ display:block; float: none; width: auto; }